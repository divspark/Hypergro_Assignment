{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node",
            "config": { "maxLambdaSize": "100mb" }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}